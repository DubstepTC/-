#include <iostream>
using namespace std;

int main() {

	setlocale(0, "");
	int a;
	cout << "Введите число: ";
	cin >> a;

	if (0 <=  a and  a <= 10) {
		cout << "Цифра в диапозоне: 1 - 10";
	}
	else if (11 <= a and a <= 20) {
		cout << "Цифра в диапозоне: 11 - 20";
	}
	else if (21 <= a and a <= 30) {
		cout << "Цифра в диапозоне: 21 - 30";
	}
	else if (31 <= a and a <= 40) {
		cout << "Цифра в диапозоне: 31 - 40";
	}
	else if (41 <= a and a <= 50) {
		cout << "Цифра в диапозоне: 41 - 50";
	}
	else if (51 <= a and a <= 60) {
		cout << "Цифра в диапозоне: 51 - 60";
	}
	else if (61 <= a and a <= 70) {
		cout << "Цифра в диапозоне: 61 - 70";
	}
	else if (71 <= a and a<= 80) {
		cout << "Цифра в диапозоне: 71 - 80";
	}
	else if (81 <= a and a <= 90) {
		cout << "Цифра в диапозоне: 81 - 90";
	}
	else if (91 <= a and a <= 100) {
		cout << "Цифра в диапозоне: 91 - 100";
	}
	else if (a > 100) {
	cout << "Ошибка! Число больше 100";
	}
	else {
		cout << "Ошибка! Число меньше 0";
	}


	int _;
	cin >> _;

	return 0;

}
